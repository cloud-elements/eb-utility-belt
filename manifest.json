{
  "manifest_version": 2,
  "name": "EB Utility Belt",
  "icons": {
    "128": "favicon.png"
  },
  "description": "This extention provides various utilities to augment Element Building",
  "version": "1.0",
  "browser_action": {
   "default_icon": "favicon.png",
   "default_popup": "index.html"
  },
  "permissions": [
	  "storage",
   	"activeTab",
    "https://api.cloud.elements.com/",
    "https://staging.cloud.elements.com/",
    "https://snapshot.cloud.elements.com/"
   ]
}
